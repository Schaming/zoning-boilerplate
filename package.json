{
  "name": "zoningbylaw",
  "version": "1.4.1",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap && next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "npm run db:start && npm run migrate && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:docker": "docker-compose up",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "db:start": "./scripts/start-db.sh",
    "db:stop": "./scripts/stop-db.sh",
    "db:reset": "docker-compose -f docker-compose.db.yml down -v && npm run dev",
    "migrate": "cross-env NODE_OPTIONS=--no-deprecation payload migrate",
    "migrate:create": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:create",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "npm run test:int && npm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation\" npx playwright test --config=playwright.config.ts",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.968.0",
    "@aws-sdk/lib-storage": "^3.968.0",
    "@payloadcms/admin-bar": "3.71.1",
    "@payloadcms/db-postgres": "3.71.1",
    "@payloadcms/live-preview-react": "3.71.1",
    "@payloadcms/next": "3.71.1",
    "@payloadcms/plugin-cloud-storage": "3.71.1",
    "@payloadcms/plugin-form-builder": "3.71.1",
    "@payloadcms/plugin-nested-docs": "3.71.1",
    "@payloadcms/plugin-redirects": "3.71.1",
    "@payloadcms/plugin-search": "3.71.1",
    "@payloadcms/plugin-seo": "3.71.1",
    "@payloadcms/richtext-lexical": "3.71.1",
    "@payloadcms/storage-s3": "3.71.1",
    "@payloadcms/ui": "3.71.1",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.1",
    "aws-crt": "^1.28.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "csvtojson": "^2.0.14",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "lucide-react": "^0.378.0",
    "next": "^15.5.9",
    "next-sitemap": "^4.2.3",
    "openai": "^6.15.0",
    "payload": "3.71.1",
    "pg": "^8.16.3",
    "prism-react-renderer": "^2.3.1",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-hook-form": "7.45.4",
    "sharp": "0.34.2",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@eslint/eslintrc": "^3.2.0",
    "@playwright/test": "1.56.1",
    "@tailwindcss/typography": "^0.5.13",
    "@testing-library/react": "16.3.0",
    "@types/escape-html": "^1.0.2",
    "@types/node": "^22.5.4",
    "@types/pg": "^8.16.0",
    "@types/react": "19.2.1",
    "@types/react-dom": "19.2.1",
    "@vitejs/plugin-react": "4.5.2",
    "autoprefixer": "^10.4.19",
    "axe-core": "^4.11.1",
    "copyfiles": "^2.4.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.7",
    "jsdom": "26.1.0",
    "playwright": "1.56.1",
    "playwright-core": "1.56.1",
    "postcss": "^8.4.38",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
